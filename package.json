    "postcss-cli": "^4.1.0",
    "postcss-at2x": "^3.2.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-custom-properties": "^5.0.2",
    "postcss-flexibility": "^1.1.0",
    "postcss-font-smoothing": "^0.1.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "7.1.3",
    "postcss-media-minmax": "^2.1.2",
